//uva-11461
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int a, b;
    while(cin >> a >> b, a+b)
    {
        int count=0;
        for(int i=a;i<=b;i++)
        {
            int temp=i;
            double x=sqrt(i);
            int x1=(int)x;
            if((x1*x1)==temp) count++;
        }
        cout << count << endl;
    }
}
