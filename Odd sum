//uva-10783
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t, count=1;
    cin >> t;
    while(t--)
    {
        cout << "Case " << count++ << ": ";
        int a, b, sum=0;
        cin >> a >> b;
        for(int i=a;i<=b;i++)
        {
            if(i%2) sum+=i;
            else continue;
        }
        cout << sum << endl;
    }
}
