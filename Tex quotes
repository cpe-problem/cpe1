//uva-00272
#include <bits/stdc++.h>
using namespace std;
string s;
int a;
int main()
{
  while(getline(cin, s))
  {
    for(int i=0;i<s.size();i++)
    {
      if(s[i]=='\"')
      {
        if(a%2==0) cout << "``";
        else cout << "''";
        a++;
      }
      else cout << s[i];
    }
    cout << endl;
  }
}
